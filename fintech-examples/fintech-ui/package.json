{
  "name": "fintech-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "openapi-gen": "openapi-generator generate -g typescript-angular -o src/app/api -i ../fintech-api/src/main/resources/static/fintech_api.yml --skip-validate-spec",
    "start": "npm run serve",
    "serve": "ng serve --port=4444 --proxy-config=proxy.conf.json",
    "serve:local": "NODE_OPTIONS=--openssl-legacy-provider ng serve --port 4444",
    "serve:dev": "ng serve --port=4444 --proxy-config=proxy-conf-dev-backend.js",
    "serve:docker": "ng serve --port=24200 --proxy-config=proxy-docker.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged --pattern 'fintech-examples/fintech-ui/**/*'",
    "format:fix-from-root:pattern": "pretty-quick --pattern 'fintech-examples/fintech-ui/**/*'"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1703.13",
    "@angular-devkit/core": "^17.3.13",
    "@angular-devkit/schematics": "^17.3.13",
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/localize": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "@popperjs/core": "^2.11.8",
    "@schematics/update": "^0.1102.19",
    "angular-iban": "^17.0.0",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^4.6.1",
    "font-awesome": "^4.7.0",
    "iban": "0.0.14",
    "jquery": "^3.6.0",
    "ng-http-loader": "^14.0.0",
    "ng2-simple-timer": "^11.2.9",
    "npm": "^10.9.2",
    "popper.js": "^1.16.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.13",
    "@angular/cdk": "^17.3.10",
    "@angular/cli": "^17.3.13",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@ngtools/webpack": "^17.3.13",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@schematics/angular": "^17.3.13",
    "@types/jasmine": "^5.1.7",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.10.2",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "^5.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "pretty-quick": "^3.0.2",
    "protractor": "^7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.2",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  }
}
