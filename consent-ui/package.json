{
  "name": "consent-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "webdriver-update": "webdriver-manager update",
    "openapi-gen": "openapi-generator generate -g typescript-angular -o src/app/api -i ../opba-consent-rest-api/src/main/resources/static/tpp_consent_api.yml",
    "openapi-auth-gen": "openapi-generator generate -g typescript-angular -o src/app/api-auth -i ../opba-auth-rest-api/src/main/resources/static/tpp_auth_api.yml",
    "start": "ng serve",
    "serve:local": "NODE_OPTIONS=--openssl-legacy-provider ng serve --proxy-config=proxy-conf-local-backend.json",
    "serve:dev": "ng serve --proxy-config=proxy-conf-dev-backend.js",
    "serve:docker": "ng serve --port=14200 --proxy-config=proxy-conf-docker-backend.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged --pattern 'consent-ui/**/*'",
    "format:fix-from-root:pattern": "pretty-quick --pattern 'consent-ui/**/*'"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "0.1303.11",
    "@angular-devkit/core": "13.3.11",
    "@angular/animations": "13.4.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "13.4.0",
    "@angular/compiler": "13.4.0",
    "@angular/core": "13.4.0",
    "@angular/forms": "13.4.0",
    "@angular/platform-browser": "13.4.0",
    "@angular/platform-browser-dynamic": "13.4.0",
    "@angular/router": "13.4.0",
    "@ngtools/webpack": "^13.3.11",
    "angular-iban": "^2.0.4",
    "angular2-uuid": "^1.1.1",
    "angularx-qrcode": "^12.0.3",
    "bootstrap": "^4.6.1",
    "iban": "0.0.14",
    "jquery": "^3.6.0",
    "ng-http-loader": "^7.0.0",
    "ng-swagger-gen": "^2.3.1",
    "ng2-simple-timer": "^9.1.0",
    "ngx-chiptan": "^0.1.4",
    "popper.js": "^1.16.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "uuidv4": "^6.2.12",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.11",
    "@angular/cli": "13.3.11",
    "@angular/compiler-cli": "13.4.0",
    "@angular/language-service": "13.4.0",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.10.2",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.1",
    "pretty-quick": "^3.0.2",
    "protractor": "^7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
